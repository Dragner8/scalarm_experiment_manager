%table#running_simulations_table
  %caption
    = t('experiments.monitoring.running_simulations_table.caption')
  %thead
    %th #
    %th
      = t('experiments.monitoring.running_simulations_table.started_at')
    %th
      = t('experiments.monitoring.running_simulations_table.results_header')
    - @experiment.range_arguments.each do |parameter_uid|
      %th= @experiment.input_parameter_label_for(parameter_uid)
