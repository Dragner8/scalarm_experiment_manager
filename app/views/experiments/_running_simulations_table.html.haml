%table#running_simulations_table
  %caption
    %h4.subheader= t('experiments.monitoring.running_simulations_table.caption')
  %thead
    %th #
    %th= t('experiments.monitoring.running_simulations_table.started_at')
    %th= t('experiments.monitoring.running_simulations_table.results_header')
    -#- @experiment.range_arguments.each do |parameter_uid|
    -#  %th= @experiment.input_parameter_label_for(parameter_uid)
  %tbody
    - @experiment.simulation_runs.where(to_sent: false, is_done: false).each do |sim_run|
      - unless sim_run.sent_at and sim_run.index and sim_run.values
        - next
      %tr(sim_index="#{sim_run.index}")
        %td.index= sim_run.index
        %td.time= sim_run.sent_at.nil? ? 'N/A' : sim_run.sent_at.strftime('%Y-%m-%d %H:%M')
        %td.results= sim_run.tmp_result.nil? ? 'N/A' : sim_run.tmp_result.to_json.to_s
