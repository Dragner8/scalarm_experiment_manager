= # socky :channels => "regression_tree_chart_monitoring"
= javascript_tag "var rt_chart_tab = new Array(); var rt_vis_tab = new Array();"

#regression_charts_form
  %h2 Regression trees
  = form_tag({:action=>:add_regression_tree_chart}, :onsubmit => "$('#busy_rt').show();", :remote => true) do
    = hidden_field_tag :experiment_id, @experiment.id
    %span
      Select MoE:
      = select_tag("moe_name", "", {:class => "moe_list"})
      
    = submit_tag "Add chart", :class => "nice_button", :style => "float: none;"
    = image_tag "loading.gif", :size => "16x16", :style => "display: none", :id => "busy_rt"
#regression_charts