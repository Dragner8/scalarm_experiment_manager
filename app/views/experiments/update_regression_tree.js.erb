el_id = "<%= "#{@experiment.id}_#{@moe_name}" %>";

<% if @tree_nodes.nil? then %>
  $("#infovis_" + el_id).html(
    "<h4>Could not create regression tree for <%= @moe_name %></h4>"
  );
<% else %>
  var json_<%= "#{@experiment.id}_#{@moe_name}" %> = <%= json_rt_node(@tree_nodes[1], @tree_nodes) %>;
  st = rt_vis_tab[el_id];
  st.loadJSON(json_<%= "#{@experiment.id}_#{@moe_name}" %>);
  st.compute();
  st.onClick(st.root);
  st.refresh();
<% end %>
// scroll to
var targetOffset = $('#infovis_'  + el_id).parent().parent().offset().top;
$('html,body').animate({scrollTop: targetOffset}, 1000);